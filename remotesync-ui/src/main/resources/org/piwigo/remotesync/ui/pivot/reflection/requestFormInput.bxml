<remotesync:RequestFormInput xmlns:bxml="http://pivot.apache.org/bxml"
	xmlns:remotesync="org.piwigo.remotesync.ui.pivot.reflection" xmlns="org.apache.pivot.wtk">
	<Checkbox bxml:id="sendCheckBox" tooltipText="send" preferredWidth="20" />
	<Label bxml:id="parameterNameLabel" text="parameterName" preferredWidth="120" />
	<TextInput bxml:id="input" preferredWidth="80" />
	<PushButton bxml:id="addInputButton" buttonData="+" preferredWidth="20" />
	<PushButton bxml:id="removeInputButton" buttonData="-" preferredWidth="20" />
</remotesync:RequestFormInput>
